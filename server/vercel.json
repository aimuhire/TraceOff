{
  "version": 2,
  "name": "traceoff-server",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "npm run build",
  "routes": [
    { "src": "/api/(.*)", "dest": "dist/index.js" },
    { "src": "/admin/(.*)", "dest": "dist/index.js" },
    { "src": "/health", "dest": "dist/index.js" },
    { "src": "/", "dest": "dist/index.js" }
  ]
}
